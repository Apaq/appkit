{
  "name": "@webstore/dev",
  "version": "0.0.1",
  "description": "Webstore - Apps for web",
  "main": "dist/index.cjs.js",
  "module": "dist/custom-elements/index.js",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/custom-elements/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/xenads/xenads.js",
  "files": [
    "dist/",
    "loader/"
  ],
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com:apaq/webstorejs.git"
  },
  "scripts": {
    "build-stencil": "cd packages/elements && stencil build --docs --docs-readme",
    "build": "npm run build-stencil",
    "dev": "stencil build --dev --docs --watch --serve --port 4001 --no-open",
    "lint": "eslint src/**/*{.ts,.tsx}",
    "prettier": "prettier --write --loglevel warn .",
    "prebuild": "npm run prettier && npm run lint",
    "serve": "node dev-server.js",
    "start": "concurrently --kill-others \"npm run dev\" \"npm run serve\"",
    "test": "stencil test --spec",
    "test.watch": "stencil test --spec --watchAll",
    "test.e2e": "stencil test --e2e ",
    "generate": "stencil generate",
    "make-dist": "node make-dist.js",
    "postbuild": "npm run make-dist",
    "version": "npm run build"
  },
  "devDependencies": {
    "@stencil/angular-output-target": "^0.0.3",
    "@stencil/eslint-plugin": "^0.3.1",
    "@stencil/sass": "^1.4.0",
    "@types/jest": "^26.0.15",
    "@types/jest-image-snapshot": "3.1.0",
    "@types/puppeteer": "^5.4.0",
    "@typescript-eslint/eslint-plugin": "^4.8.0",
    "@typescript-eslint/parser": "^4.8.0",
    "browser-sync": "^2.26.13",
    "chalk": "^4.0.0",
    "concurrently": "^5.1.0",
    "del": "^5.1.0",
    "eslint": "^7.13.0",
    "eslint-plugin-react": "^7.21.5",
    "express": "^4.17.1",
    "front-matter": "^4.0.2",
    "glob": "^7.1.6",
    "http-proxy": "^1.18.1",
    "http-proxy-middleware": "^1.0.6",
    "husky": "^4.2.5",
    "jest": "26.6.3",
    "jest-cli": "26.6.3",
    "jest-image-snapshot": "4.1.0",
    "prettier": "2.0.5",
    "puppeteer": "^5.4.1",
    "recursive-copy": "^2.0.10",
    "through2": "^3.0.1",
    "typescript": "^4.0.5",
    "workbox-build": "4.3.1"
  },
  "license": "MIT",
  "dependencies": {
    "@stencil/core": "^2.0.1",
    "@popperjs/core": "^2.5.4",
    "date-fns": "^2.16.1"
  }
}
